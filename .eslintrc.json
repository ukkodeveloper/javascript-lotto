{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": ["warn"],
    "no-console": "off",
    "no-param-reassign": ["warn", { "props": false }],
    "import/extensions": ["error", "always"],
    "class-methods-use-this": "off",
    "max-params": ["error", 2],
    "max-depth": ["error", 1],
    "max-lines-per-function": ["error", 12]
  }
}
